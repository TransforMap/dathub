{
  "name": "dat-registry",
  "version": "1.0.0",
  "description": "a registry for dats",
  "main": "index.js",
  "scripts": {
    "start": "npm run watch & npm run watch-css & npm run server",
    "server": "nodemon server.js -e js,css,html",
    "test": "node api/test/run.js | tap-spec & browserify -t ractivate app/test/index.js | tape-run",
    "watch": "watchify -t ractivate app/index.js -o static/js/bundle.js",
    "watch-css": "autoless static/styles"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/karissamck/dat-registry.git"
  },
  "keywords": [
    "data",
    "registry",
    "web"
  ],
  "author": "karissa",
  "license": "BSD",
  "bugs": {
    "url": "https://github.com/karissamck/dat-registry/issues"
  },
  "homepage": "https://github.com/karissamck/dat-registry",
  "dependencies": {
    "bcrypt": "^0.8.0",
    "body": "^4.5.0",
    "bytewise": "^0.7.1",
    "debug": "^2.0.0",
    "extend": "^2.0.0",
    "fulltext-engine": "^1.1.1",
    "github-oauth": "^0.2.0",
    "is-url": "^1.1.0",
    "jquery-browserify": "^1.8.1",
    "level-prebuilt": "^0.18.0",
    "level-queryengine": "^3.0.1",
    "level-restful": "1.0.1",
    "level-session": "^0.3.1",
    "monotonic-timestamp": "0.0.9",
    "page": "^1.4.1",
    "ractive": "^0.6.1",
    "redirecter": "^0.2.3",
    "request": "^2.45.0",
    "routes-router": "^4.1.1",
    "run-waterfall": "^1.0.2",
    "send-data": "^3.3.3",
    "st": "^0.5.2",
    "xhr": "^1.17.0"
  },
  "devDependencies": {
    "autoless": "^0.1.5",
    "ractivate": "^0.2.0",
    "rimraf": "^2.2.8",
    "tap-spec": "^1.0.1",
    "tape": "^3.0.0",
    "watchify": "^2.1.1"
  }
}
